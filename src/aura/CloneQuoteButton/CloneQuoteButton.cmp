<aura:component controller="CloneQuote" implements="force:appHostable,flexipage:availableForAllPageTypes,force:hasRecordId,force:lightningQuickAction">
    <aura:attribute name="recordId" type="Id" />
   
    <aura:attribute name="Quote" type="Quote"/>
   	<aura:handler name="init" value="{!this}" action="{!c.doInit}"/>
    
    <div class="slds-panel slds-grid slds-grid_vertical slds-nowrap slds-align_absolute-center" style="padding-top: 10px; padding-bottom: 10px">

        <lightning:button label="Clonar" onclick="{! c.createNewBoat }" />

    </div>
   

</aura:component>